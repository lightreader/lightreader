[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Lightreader (lightreader)
[access "refs/for/refs/*"]
	read = group user/Lightreader (lightreader)
	push = group user/Lightreader (lightreader)
	label-Code-Review = -2..+2 group user/Lightreader (lightreader)
	label-Verified = -1..+1 group user/Lightreader (lightreader)
	rebase = group user/Lightreader (lightreader)
	removeReviewer = group user/Lightreader (lightreader)
	submit = group user/Lightreader (lightreader)
[access "refs/heads/*"]
	read = group user/Lightreader (lightreader)
	create = group user/Lightreader (lightreader)
	push = +force group user/Lightreader (lightreader)
	pushMerge = group user/Lightreader (lightreader)
[access "refs/tags/*"]
	push = group user/Lightreader (lightreader)
	push = +force group user/Lightreader (lightreader)
